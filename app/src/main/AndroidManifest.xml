<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moviereviewer"
    android:largeHeap="true">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.Login_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.MainActivity" />
        <activity android:name=".ui.Register_Activity" />
        <activity android:name=".ui.Profile_Activity" />
        <activity android:name=".ui.Register_main_Activity" />
        <activity android:name=".ui.Profile_show_Activity" />
        <activity android:name=".test.ProfileActivity" />
        <activity android:name=".test.SignUpActivity" />
        <activity android:name=".MainTestActivity"></activity>
    </application>

</manifest>